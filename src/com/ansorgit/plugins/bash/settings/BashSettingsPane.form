        </constraints>
        <properties/>
        <border type="none"/>
        <children>
          <component id="49b61" class="javax.swing.JCheckBox" binding="loadFilesWithoutExtension">
            <constraints>
              <grid row="0" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="8" fill="0" indent="0" use-parent-layout="false"/>
            </constraints>
            <properties>
              <text value="&amp;Accept files without extension (needs restart)"/>
              <toolTipText value="If enabled files which do not have an extension are accepted by the BashSupport plugin as Bash script files."/>
